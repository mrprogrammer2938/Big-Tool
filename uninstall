clear
echo -n "uninstalling Big-Tool "
sleep 3 & while [ "$(ps a | awk '{print $1}' | grep $!)" ] ; do for X in '-' '\' '|' '/'; do echo -en "\b$X"; sleep 0.1; done; done
cd .. && sudo rm -r Big-Tool && cd /usr/share/ && sudo rm tools && cd
sleep 2
echo ""
echo ""
echo "finish!"
echo ""
echo "Good BYe :)"
echo ""
echo "Dont Forget to check Github: https://github.com/msprogrammer2938 ! :)"
echo ""
exit 1
